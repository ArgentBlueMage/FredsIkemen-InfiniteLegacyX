[Info]
name = "Dream"
displayname = "Dream"
author = "Rak√≠el"

;--------------------------------------------------------
[Camera]
startx = 0
starty = 0
boundleft = -1200
boundright = 1180
boundhigh = -450
boundlow = 0

verticalfollow = .85
floortension = 200
tension = 200

overdrawhigh = 120
overdrawlow = 120
cuthigh = 0
cutlow = 0

;Zoom is not yet supported in 3D stages.
zoomout = .8

;Field of view. 30 is good for photorealistic scenes, but feel free to tweak this to your preference.
fov = 50

yshift = -.3

near = 0.1
far = 10000
;--------------------------------------------------------
[PlayerInfo]
;--- Player 1 ---
p1startx = -280
p1starty = 0
p1facing = 1

;--- Player 2 ---
p2startx = 280
p2starty = 0
p2facing = -1

;--- Common ---
leftbound  = -4000
rightbound =  4000

;--------------------------------------------------------
[Bound]
screenleft = 60
screenright = 60

;--------------------------------------------------------
[StageInfo]
zoffset = 660

autoturn = 1
resetBG = 1

localcoord = 1280, 720
xscale = 1
yscale = 1

portraitscale = 1

;--------------------------------------------------------
[Shadow]
intensity = 96
color = 0,0,0
yscale = .3
fade.range = 0,0

;--------------------------------------------------------
[Reflection]
intensity = 0

;--------------------------------------------------------
[Music]
bgmusic = sound/stage_music/Dream.mp3
bgmvolume = 100

;========================================================
[BGdef]

;Filename of 3D model file
;Supported file types are gltf and glb; supported by most modern 3D model programs.
;It is recommended to use PNG/JPEG/embedded image format for textures. Compressed dds textures are not supported.
;Make sure the world origin of the model is at the camera starting point.
model = Dream.glb

;Filename of .sff file, required by spec (and for stage portraits)
spr = Dream.sff

;debugbg = 0

;-------------------------------------
; Model element definition; similar to [BG] blocks in 2D stages
[Model]

; X, Y, and Z coordinates of the model
offset = 0,-3,-10

; Scale of the model
scale = 5, 5, 5

