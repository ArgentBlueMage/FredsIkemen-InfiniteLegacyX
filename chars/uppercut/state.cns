; Run forward
[Statedef 100]
type    = S
physics = S
anim = 100
sprpriority = 1

[State 0, CtrlSet]
type = CtrlSet
triggerall = ctrl = 1
trigger1 = frontedgedist < 15
trigger2 = backedgedist < 15
value = 0
ignorehitpause = 1
[State 0, CtrlSet]
type = CtrlSet
triggerall = ctrl = 0
trigger1 = frontedgedist >= 15
trigger1 = backedgedist >= 15
value = 1
ignorehitpause = 1

[State 0, PlayerPush]
type = PlayerPush
trigger1 = 1
value = 0
ignorehitpause = 1

[State 0, AngleDraw]
type = AngleDraw
trigger1 = 1
value = 0
scale = .6,.6
ignorehitpause = 1

[State 0, ScreenBound]
type = ScreenBound
trigger1 = frontedgedist < 15
trigger2 = backedgedist < 15
value = 0
movecamera = 0,1
ignorehitpause = 1

[State 0, PosAdd]
type = PosAdd
trigger1 = frontedgedist < -40
x = -50 - backedgedist

[State 100, 1]
type = VelSet
trigger1 = 1
x = const(velocity.run.fwd.x)

[State 100, 2] ;Prevent run from canceling into walk
type = AssertSpecial
trigger1 = 1
flag = NoWalk

[State 100, 3] ;Prevent from turning
type = AssertSpecial
trigger1 = 1
flag = NoAutoTurn

[State 100, 4]
type = ChangeState
trigger1 = command != "holdfwd" && var(50) != 100
trigger1 = frontedgedist >= 15
trigger1 = backedgedist >= 15
trigger1 = !var(55) || animtime = 0
value = 0

